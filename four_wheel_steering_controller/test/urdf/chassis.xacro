  <link
    name="cover">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass
        value="100.38" />
      <inertia
        ixx="68.516"
        ixy="-2.1784"
        ixz="6.7594"
        iyy="357.45"
        iyz="0.25495"
        izz="375.18" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://four_wheel_steering_controller/test/meshes/cover.STL" />
      </geometry>
	<material name="silver">
		<color rgba="0.700 0.700 0.700 1.000"/>
	</material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://four_wheel_steering_controller/test/meshes/cover.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="cover_joint" type="fixed">
   <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <child link="cover" />
    <parent link="base_link" />
    </joint>  
    
